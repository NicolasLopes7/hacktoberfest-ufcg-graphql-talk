{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "prisma:check": {
            "dependsOn": [
                "^prisma:check"
            ],
            "outputs": []
        },
        "dev": {
            "dependsOn": [
                "prisma:check",
                "^dev"
            ],
            "outputs": []
        }
    }
}