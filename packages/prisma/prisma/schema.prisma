generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Pokemon {
  id        Int    @id @default(autoincrement())
  name      String
  image_url String

  pokemonInfoId Int?
  info          PokemonInfo[]
}

model PokemonInfo {
  id        Int     @id @default(autoincrement())
  type      String
  pokemonId Int
  pokemon   Pokemon @relation(fields: [pokemonId], references: [id])
}
